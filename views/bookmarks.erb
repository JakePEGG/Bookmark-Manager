<ul>
  <% @bookmarks.each do |bookmark| %>
    <li>
      <a href="<%= bookmark.url %>">
        <%= bookmark.title %>
      </a>
      <form action="/delete/<% bookmark.url%>/<% bookmark.title %>" method="post">
      <input name="_method" type="hidden" value="delete" />
      <input type="submit" value="Delete" class="btn btn-outline-primary">
      </form>
    </li>
  <% end %>
</ul>
